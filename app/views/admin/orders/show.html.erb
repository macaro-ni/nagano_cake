<h5>注文履歴一覧</h5>

<id class="container">
  <id class="row">
    <table class="table col-md-12">
      <thead>
        <tr class="thead-light">
          <th>購入日時</th>
        
          <th>注文個数</th>

        </tr>
      </thead>
      <tbody>
        <% @orders.each do |order| %>
        <tr>
            <td><%= order.created_at.strftime('%Y/%m/%d %H:%M') %></td>
           
            <td><!--%= order.order_detail.amount %--></td>

        </tr>
        <% end %>
      </tbody>
    </table>
  </id>
</id>


<%= paginate @orders %>