<h5>商品一覧</h5>

<table mx-auto>
  <tr>
    <th>商品ID</th>
    <th>商品名</th>
    <th>税抜価格</th>
    <th>ステータス</th>
  </tr>
  <tr>
    <% @items.each do |item| %>
    <td><%= item.id %></td>
    <td><%= item.name %></td>
    <td><%= item.price %></td>
    <td><%= item.is_active %></td>
    <% end %>
  </tr>
</table>

<%= paginate items %>